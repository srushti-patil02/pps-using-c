#include<stdio.h>
int main ()
{
   int a,b;
   
   printf("Enter the num1 \t");
   scanf("%d",&a);
   
   printf("Enter the num2 \t");
   scanf("%d",&b);
   
   printf("Addition of a and b is %d",sum(a,b));
   return 0;
}
   int sum (int x,int y)
   {
      int c;
      c=x+y;
      
      return c;
   }  
